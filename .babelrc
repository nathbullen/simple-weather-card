{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": "> 5%, not dead"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": false, "version": "2023-05" }],
    ["@babel/plugin-transform-class-properties"]
  ]
}
